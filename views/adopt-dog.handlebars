
    <h2>Available Dogs for Adoption</h2>
    <div id="dog-gallery" class="row">
      {{> dogPartial dogs=dogs}}
    </div>

  <!-- Handlebars Partial Template for Dog Cards -->
  <script id="dog-partial-template" type="text/x-handlebars-template">
    {{#each dogs}}
      <div class="col-md-4 mb-4">
        <div class="card">
          <img src="/images/{{this.image}}" class="card-img-top" alt="{{this.name}}">
          <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <p class="card-text">Owner: {{this.owner}}</p>
            <p class="card-text">Sex: {{this.sex}}</p>
            <p class="card-text">Birth Date: {{this.birth}}</p>
          </div>
        </div>
      </div>
    {{/each}}
  </script>

